    <script type="text/ng-template" id="SupersizedView.cshtml">
    
<div id="supersized-container" ng-class="{'fade-controls': mouseStopped}">
    <div class="supersized-debugger" style="display: none;">
        <div>Max Zoom = {{hmr.maxZoom}}</div>
        <div>Heigth = {{model.currentImage.height}}</div>
        <div>Width = {{model.currentImage.width}}</div>
    </div>
    <div class="sueprsized-navigation-left"><a class="sueprsized-navigation-fullscreen-prev" hm-tap="swipeHandler('prev')"></a></div>
    <div class="sueprsized-navigation-fullscreen-top"></div>
    <div class="sueprsized-navigation-fullscreen-bottom"></div>
    <div class="overlay-panel" ng-show="hmr.currentMoves.pinch">
        <strong>{{100 * (hmr.transform[0] / hmr.maxZoom) | number:0}}%</strong>
    </div>
    <ul ng-show="imageVariantsVisible" class="supersized-variants-container" ng-class="{'open': imageVariantsOpen, 'animate': imageVariantsOpenAnimation}">
        <li class="controls">
            <a class="supersized-navigation-variants-arrow" ng-if="model.artwork.images" hm-tap="toggleImageVariants()"></a>
            <a class="supersized-navigation-variants" ng-if="model.artwork.images" hm-tap="toggleImageVariants()" ng-class="{'open': scope.imageVariantsOpen}">{{model.artwork.images.length + 1}}</a>
        </li>
        <li ng-class="model.variantIndex == -1 ? 'selected' : ''" style="background-image: url('{{imageVariantUrl(model.artwork.image)}}')" ng-click="$root.$broadcast('supersized-variant', {index: -1})"></li>
        <li ng-class="model.variantIndex == $index ? 'selected' : ''" ng-repeat="img in model.artwork.images" style="background-image: url('{{imageVariantUrl(img.image)}}')" ng-click="$root.$broadcast('supersized-variant', {index: $index})"></li>
    </ul>
    <div class="supersized-content">
        <div class="size-debugger" ng-show="showDebugger">
            <table>
                <thead>
                    <t>
                <td>Image</td>
                <td>Size</td>
                <td>State</td>
                </t>
                </thead>
                <tbody>
                    <tr ng-repeat="(k, v) in imagesPreloader">
                        <td>{{v.name}}</td>
                        <td>{{imageSizeService.getSizeName(k)}}</td>
                        <td>{{v.state}}</td>
                    </tr>
                </tbody>
            </table>
            <div class="debugger-flags">
                <div class="debugger-flag" ng-repeat="(k, v) in cfg.PAINTING_FOR_GALLERY_FLAGS" ng-class="{'active': flagIsChecked(model.artwork.flags, v)}">
                    {{k}}
                </div>
            </div>
            <a ng-click="hideDebugger()">Close</a>
        </div>
        <a class="hidden-blank"
           ng-dblclick="redirectToOrderReprudution()">
            <div class="supersized-slide-wrapper" ng-class="{'grab': grabCursor(), 'pointer': pointerCursor()}"
                 ng-swipe-left="swipeleft()"
                 ng-swipe-right="swiperight()"
                 hm-panstart="panstart($event)"
                 hm-panmove="panmove($event)"
                 hm-panend="panend($event)"
                 hm-pinchstart="pinchstart($event)"
                 hm-pinchmove="pinchmove($event)"
                 hm-pinchend="pinchend($event)"
                 hm-doubletap="doubletap($event)"
                 hm-tap="tap($event)"
                 hm-recognizer-options='[{"type":"tap", event: "doubletap", "enable":true,"taps":2,"time":500,"interval":250,"threshold":10,"posThreshold":200}]'>
                <img class="primary-image" ng-class="{'loading' : model.loading }" /><div class="loader" ng-class="{'visible' : originalIsLoading && zoomChanged }"></div>
                <div class="copyright-message" ng-if="showCopyrightText()" ng-bind="buildCopyrightText()"></div>
                <div><img class="animation-helper"></div>
